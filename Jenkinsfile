pipeline {
    agent any 

    stages {
        stage("Build Java Release") {
            steps {
                echo "Hello Build Working Java Release" 
            }
        }

        stage("Test Java Release") {
            steps {
                echo "Testing Working Java Release"
            }
        }

      stage("Deploy Java Release") {
            steps {
                echo "Deploy Working Java Release"
            }
        }
    }
    
}
