pipeline {
    agent any 

    stages {
        stage("Build Java Developer") {
            steps {
                echo "Hello Build Working Java Developer" 
            }
        }

        stage("Test Java Developer") {
            steps {
                echo "Testing Working Java Developer"
            }
        }

      stage("Deploy Java Developer") {
            steps {
                echo "Deploy Working Java Developer"
            }
        }
    }
    
}
