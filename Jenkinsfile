    agent any 

    stages {
        stage("Build Java Test") {
            steps {
                echo "Hello Build Working Java Test" 
            }
        }

        stage("Test Java Test") {
            steps {
                echo "Testing Working Java Test"
            }
        }

      stage("Deploy Java Test") {
            steps {
                echo "Deploy Working Java Test"
            }
        }
    }
    
}
