            steps {
                echo "Hello Build Working Java" 
            }
        }

        stage("Test Java") {
            steps {
                echo "Testing Working Java"
            }
        }

      stage("Deploy Java") {
            steps {
                echo "Deploy Working Java"
            }
        }
    }
    
}
